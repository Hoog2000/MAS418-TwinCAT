    <Method Name="Joystick" Id="{2938f7a2-aab4-4c3c-a932-262a2d51b521}">
      <Declaration><![CDATA[METHOD Joystick
VAR_INPUT
	bSwitch	: BOOL;
	bActive : BOOL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT(bActive) THEN
	G_Inputs.fJoyY := 0.0;
	G_Inputs.fJoyX := 0.0;
	G_VB.fJoyX := 0.0;
	G_VB.fJoyY := 0.0;
ELSIF (bSwitch) THEN
	G_Inputs.fJoyY := G_VB.fJoyY;
	G_Inputs.fJoyX := 0.0;
	G_VB.fJoyX := 0.0;
ELSIF NOT(bSwitch) THEN
	G_Inputs.fJoyY := 0.0;
	G_Inputs.fJoyX := G_VB.fJoyX;
	G_VB.fJoyY := 0.0;
END_IF]]></ST>
      </Implementation>
    </Method>
    <Method Name="Mode" Id="{cbaf3c46-ffe3-4434-b8af-272018a9ab02}">
      <Declaration><![CDATA[METHOD Mode
